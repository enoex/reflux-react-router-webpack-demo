{
  "name": "reflux-react-router-webpack-demo",
  "version": "1.0.0",
  "description": "A demo showing how to use Reflux in conjunction with react-router",
  "scripts": {
    "local": "webpack --color --progress --watch",
    "dev": "webpack-dev-server --port 8124 --colors",
    "test": "node --harmony ./node_modules/jest-cli/bin/jest --coverage"
  },
  "bin": {
    "webpack": "./bin/webpack",
    "webpack-dev-server": "./bin/webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/roundrobin/reflux-react-router-webpack-demo.git"
  },
  "keywords": [
    "react",
    "react-router",
    "reflux"
  ],
  "author": "Nils Schlomann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/roundrobin/reflux-react-router-webpack-demo/issues"
  },
  "homepage": "https://github.com/roundrobin/reflux-react-router-webpack-demo",
  "dependencies": {
    "babel-jest": "^5.2.0",
    "babel-loader": "5.1.3",
    "bragi-browser": "0.0.11",
    "classnames": "^2.1.2",
    "css-loader": "0.14.4",
    "extract-text-webpack-plugin": "0.8.1",
    "immutable": "^3.7.3",
    "jquery": "^2.1.4",
    "lodash": "^3.9.3",
    "node-libs-browser": "0.5.2",
    "node-sass": "3.1.2",
    "react": "^0.13.3",
    "react-immutable-render-mixin": "^0.8.1",
    "react-router": "0.13.3",
    "react-tools": "^0.13.3",
    "reflux": "^0.2.7",
    "sass": "0.5.0",
    "sass-loader": "1.0.2",
    "style-loader": "0.12.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "devDependencies": {
    "babel-jest": "^5.2.0",
    "jest-cli": "^0.4.10"
  },
  "jest": {
    "testDirectoryName": "__tests__",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "../node_modules/react",
      "../node_modules/react/addons",
      "../node_modules/immutable"
    ],
    "collectCoverage": true,
    "collectCoverageOnlyFrom" : {
      "./static/js/components/MembersList.jsx" : true
    }
  }
}
